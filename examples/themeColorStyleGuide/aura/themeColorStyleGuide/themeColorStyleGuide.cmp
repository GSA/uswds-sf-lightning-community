<aura:component
  implements="flexipage:availableForAllPageTypes,forceCommunity:availableForAllPageTypes"
  access="global"
>
  <aura:attribute name="profileMenu" type="Aura.Component[]" required="false" />

  <aura:handler name="init" value="{!this}" action="{!c.onInit}" />

  <p>
    background - if icons do not appear in the left hand column, please be sure
    to add the FontAwesome.js files.
  </p>
  <!-- this is a template card-->
  <article class="slds-card">
    <div class="slds-card__header slds-grid">
      <header class="slds-media slds-media_center slds-has-flexi-truncate">
        <div class="slds-media__body">
          <h2 class="slds-card__header-title">
            <a
              href="#"
              class="slds-card__header-link slds-truncate"
              title="Accounts"
            >
              <span>TITLE</span>
            </a>
          </h2>
        </div>
      </header>
    </div>
    <div class="slds-card__body slds-card__body_inner">Card Body</div>
  </article>
  <!-- END template-->
  <article class="slds-card">
    <div class="slds-card__header slds-grid">
      <header class="slds-media slds-media_center slds-has-flexi-truncate">
        <div class="slds-media__body">
          <h2 class="slds-card__header-title">
            <a
              href="#"
              class="slds-card__header-link slds-truncate"
              title="Accounts"
            >
              <span>Theme Colors</span>
            </a>
          </h2>
        </div>
      </header>
    </div>
    <div class="slds-card__body slds-card__body_inner">
      <div class="slds-card__body slds-card__body_inner">
        <p>
          References:
          <ul>
            <li>
              <a
                href="https://www.lightningdesignsystem.com/design-tokens/#category-text-color"
                >https://www.lightningdesignsystem.com/design-tokens/#category-text-color</a
              >
            </li>
            <li>
              <a
                href="https://developer.salesforce.com/docs/atlas.en-us.lightning.meta/lightning/tokens_standard_communities.htm"
                >https://developer.salesforce.com/docs/atlas.en-us.lightning.meta/lightning/tokens_standard_communities.htm</a
              >
            </li>
          </ul>
        </p>
        <table class="slds-table slds-table_cell-buffer slds-table_bordered">
          <thead>
            <tr>
              <th>Color</th>
              <th>Theme Branding Name</th>
              <th>Maps to these design tokens</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>
                <i
                  aura:id="faicon"
                  class="fas fa-circle fa-3x COLOR_TEXT_DEFAULT"
                ></i>
              </td>
              <td><span>Text Color</span></td>
              <td>
                <ul>
                  <li>colorTextDefault</li>
                </ul>
              </td>
            </tr>
            <tr>
              <td>
                <i
                  aura:id="faicon"
                  class="fas fa-circle fa-3x DETAIL_TEXT_COLOR"
                ></i>
              </td>
              <td><span>Detail Text Color</span></td>
              <td>
                <ul>
                  <li>colorTextActionLabel</li>
                  <li>colorTextLabel</li>
                  <li>colorTextPlaceholder</li>
                  <li>colorTextWeak</li>
                </ul>
              </td>
            </tr>
            <tr>
              <td>
                <span class="dot ACTION_COLOR"></span>
              </td>
              <td><span>Action Color</span></td>
              <td>
                <ul>
                  <li>colorBackgroundButtonBrand</li>
                  <li>colorBorderBrand</li>
                  <li>colorBorderButtonBrand</li>
                  <li>colorBrand</li>
                  <li>colorTextBrand</li>
                  <li>colorTextTabLabelSelected</li>
                  <li>colorTextActionLabelActive</li>
                </ul>
              </td>
            </tr>
            <tr>
              <td>
                <i class="fas fa-circle fa-3x COLOR_TEXT_LINK"></i>
              </td>
              <td><span>Link Color</span></td>
              <td>
                <ul>
                  <li>colorTextLink</li>
                </ul>
              </td>
            </tr>
            <tr>
              <td>
                <span class="fa-stack fa-2x" style="vertical-align: top"
                  ><i class="far fa-circle fa-stack-2x BLACK"></i
                  ><i class="fas fa-circle fa-stack-1x OVERLAY_TEXT_COLOR"></i
                ></span>
              </td>
              <td><span>Overlay Text Color</span></td>
              <td>
                <ul>
                  <li>colorTextButtonBrand</li>
                  <li>colorTextButtonBrandHover</li>
                  <li>colorTextInverse</li>
                </ul>
              </td>
            </tr>

            <tr>
              <td>
                <span class="dot BORDER_COLOR"></span>
              </td>
              <td><span>Border Color</span></td>
              <td>
                <ul>
                  <li>colorBorder</li>
                  <li>colorBorderButtonDefault</li>
                  <li>colorBorderInput</li>
                  <li>colorBorderSeparatorAlt</li>
                </ul>
              </td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </article>

  <!-- Buttons -->
  <article class="slds-card">
    <div class="slds-card__header slds-grid">
      <header class="slds-media slds-media_center slds-has-flexi-truncate">
        <div class="slds-media__body">
          <h2 class="slds-card__header-title">
            <a
              href="#"
              class="slds-card__header-link slds-truncate"
              title="Accounts"
            >
              <span>Buttons</span>
            </a>
          </h2>
        </div>
      </header>
    </div>
    <div class="slds-card__body slds-card__body_inner">
      <p>
        <a href="https://www.lightningdesignsystem.com/components/buttons/"
          >https://www.lightningdesignsystem.com/components/buttons/</a
        >
      </p>
      <button class="slds-button">Button</button>
      <button class="slds-button slds-button_neutral">Neutral Button</button>
      <button class="slds-button slds-button_brand">Brand Button</button>
      <button class="slds-button slds-button_outline-brand">
        Outline Brand Button
      </button>
      <button class="slds-button slds-button_destructive">
        Destructive Button
      </button>
      <button class="slds-button slds-button_text-destructive">
        Text Destructive Button
      </button>
      <button class="slds-button slds-button_success">Success Button</button>

      <pre>
        <code>
          &lt;button class="slds-button"&gt;Button&lt;/button&gt; 
          &lt;button class="slds-button slds-button_neutral"&gt;Neutral Button&lt;/button&gt; 
          &lt;button class="slds-button slds-button_brand"&gt;Brand Button&lt;/button&gt; 
          &lt;button class="slds-button slds-button_outline-brand"&gt;Outline Brand Button&lt;/button&gt; 
          &lt;button class="slds-button slds-button_destructive"&gt;Destructive Button&lt;/button&gt;
          &lt;button class="slds-button slds-button_text-destructive"&gt;Text Destructive Button&lt;/button&gt; 
          &lt;button class="slds-button slds-button_success"&gt;Success Button&lt;/button&gt;
        </code>
      </pre>
    </div>
  </article>

  <article class="slds-card">
    <div class="slds-card__header slds-grid">
      <header class="slds-media slds-media_center slds-has-flexi-truncate">
        <div class="slds-media__body">
          <h2 class="slds-card__header-title">
            <a
              href="#"
              class="slds-card__header-link slds-truncate"
              title="Accounts"
            >
              <span>RFS Button Examples</span>
            </a>
          </h2>
        </div>
      </header>
    </div>
    <div class="slds-card__body slds-card__body_inner">
      <div>
        <h3>RFS Disabled</h3>
        <lightning:button
          variant="brand"
          type="button"
          name="Submit"
          label="Submit"
          class="slds-m-top_medium"
          disabled="True"
        />&nbsp;
        <lightning:button
          variant="brand"
          type="button"
          name="Share Record"
          label="Share Record"
          class="slds-m-top_medium"
          disabled="True"
        />&nbsp;
        <lightning:button
          variant="brand"
          type="button"
          name="Help"
          label="Help"
          class="slds-m-top_medium"
          disabled="False"
        />
      </div>
      <div>
        <h3>RFS Editing Window</h3>
        <lightning:button
          variant="brand"
          type="submit"
          name="Save"
          label="Save"
          class="slds-m-top_medium"
        />&nbsp;
        <lightning:button
          variant="brand"
          type="submit"
          name="Cancel"
          label="Cancel"
          class="slds-m-top_medium"
        />
      </div>
      <div>
        <h3>RFS Not Submitted, Not Editing</h3>
        <lightning:button
          variant="brand"
          type="button"
          name="Edit"
          label="Edit"
          class="slds-m-top_medium"
        />&nbsp;
        <lightning:button
          variant="brand"
          type="button"
          name="Submit"
          label="Submit"
          class="slds-m-top_medium"
        />&nbsp;
        <lightning:button
          variant="brand"
          type="button"
          name="Share Record"
          label="Share Record"
          class="slds-m-top_medium"
        />&nbsp;
        <lightning:button
          variant="brand"
          type="button"
          name="Help"
          label="Help"
          class="slds-m-top_medium"
        />
      </div>
    </div>
  </article>
  <!-- ROE Buttons -->
  <article class="slds-card">
    <div class="slds-card__header slds-grid">
      <header class="slds-media slds-media_center slds-has-flexi-truncate">
        <div class="slds-media__figure">
          <span> <span class="slds-assistive-text">account</span> </span>
        </div>
        <div class="slds-media__body">
          <h2 class="slds-card__header-title">
            <a
              href="#"
              class="slds-card__header-link slds-truncate"
              title="Accounts"
            >
              <span>ROE Buttons</span>
            </a>
          </h2>
        </div>
      </header>
    </div>
    <div class="slds-card__body slds-card__body_inner">
      <h3>SF118_Status NOT EQUAL Submitted</h3>
      <p>
        <ui:inputCheckbox aura:id="complete" value="false" /> &nbsp;Mark as
        Complete
      </p>
      <lightning:button
        class="slds-theme_neutral"
        variant="brand"
        label="New"
      ></lightning:button>
      <lightning:button
        class="slds-theme_neutral"
        iconName="utility:delete"
        iconPosition="left"
        variant="destructive"
        disabled="false"
        label="Delete"
      ></lightning:button>
      <lightning:button
        class="slds-theme_neutral"
        variant="brand"
        label="Import"
      ></lightning:button>
      <Lightning:button
        class="slds-theme_neutral"
        variant="brand"
        label="Download Template"
      />
      <Lightning:button
        class="slds-theme_neutral"
        variant="brand"
        label="Upload Help"
      />
    </div>
  </article>

  <!-- ROE Tabs-->
  <article class="slds-card">
    <div class="slds-card__header slds-grid">
      <header class="slds-media slds-media_center slds-has-flexi-truncate">
        <div class="slds-media__figure">
          <span> <span class="slds-assistive-text">account</span> </span>
        </div>
        <div class="slds-media__body">
          <h2 class="slds-card__header-title">
            <a
              href="#"
              class="slds-card__header-link slds-truncate"
              title="Accounts"
            >
              <span>ROE Tabs</span>
            </a>
          </h2>
        </div>
      </header>
    </div>
    <div class="slds-card__body slds-card__body_inner">
      <lightning:tabset
        aura:id="primarytabs1"
        variant="scoped"
        class="slds-tabs_large slds-tabs_card"
        selectedTabId="SF118_Tab"
      >
        <lightning:tab id="SF118_Tab" label="SF118" class="slds-show">
          <lightning:layout> </lightning:layout>
        </lightning:tab>
        <lightning:tab id="Schedule_A_Tab" label="Schedule A">
          <div></div>
        </lightning:tab>
        <lightning:tab id="Schedule_B_Tab" label="Schedule B">
          <div></div>
        </lightning:tab>
        <lightning:tab id="Schedule_C_Tab" label="Schedule C">
          <div></div> </lightning:tab
      ></lightning:tabset>
    </div>
  </article>

  <article class="slds-card">
    <div class="slds-card__header slds-grid">
      <header class="slds-media slds-media_center slds-has-flexi-truncate">
        <div class="slds-media__figure">
          <span> <span class="slds-assistive-text">account</span> </span>
        </div>
        <div class="slds-media__body">
          <h2 class="slds-card__header-title">
            <a
              href="#"
              class="slds-card__header-link slds-truncate"
              title="Accounts"
            >
              <span>Profile Menu</span>
            </a>
          </h2>
        </div>
      </header>
    </div>
    <div class="slds-card__body slds-card__body_inner">
      <h3>Login Link</h3>
      <div>
        <div class="ui-widget">
          <div
            class="login-wrapper selfServiceUserProfileMenu"
            data-aura-class="selfServiceUserProfileMenu"
          >
            <div class="login">
              <a class="login-link" title="Login" href="javascript:void(0);"
                ><span class="linkLabel">Login</span></a
              >
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="slds-card__body slds-card__body_inner">
      <h3>User Profile Menu Link</h3>
      <div data-region-name="profileMenu">
        <div class="ui-widget" data-priority="">
          <div
            class="uiMenu selfServiceUserProfileMenu"
            data-aura-class="uiMenu selfServiceUserProfileMenu"
          >
            <div
              id="3:58;a"
              class="uiPopupTrigger selfServiceProfileMenuTrigger"
              data-aura-class="uiPopupTrigger selfServiceProfileMenuTrigger"
              data-interactive-uid="3"
            >
              <div>
                <div>
                  <a
                    class="trigger-link"
                    aria-expanded="false"
                    aria-haspopup="true"
                    aria-label="Mark Vogelgesang"
                    href="javascript:void(0);"
                  >
                    <img
                      class="profileIcon"
                      alt=""
                      src="/profilephoto/005/T"
                      title="Mark Vogelgesang"
                    />
                    <span class="profileName">Mark Vogelgesang</span>
                    <span class="triggerDownArrow down-arrow"></span>
                  </a>
                </div>
              </div>
            </div>
            <div
              class="menuList popupTargetContainer uiPopupTarget uiMenuList uiMenuList--default"
              data-aura-class="uiPopupTarget uiMenuList uiMenuList--default"
            >
              <div class="menuList" role="menu">
                <ul class="scrollable" role="presentation">
                  <li></li>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </article>
</aura:component>
